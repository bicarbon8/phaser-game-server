{
  "name": "usage-example",
  "version": "1.1.0",
  "description": "example usage of the phaser-game-server projects",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && webpack",
    "start": "npm run build && npm run open",
    "start:dev": "npm run build && npm run open:dev",
    "open": "npx phaser-game-server -s -c server.config.prod.json",
    "open:dev": "npx phaser-game-server -s",
    "test": ""
  },
  "keywords": [
    "phaser",
    "html game",
    "typescript game",
    "example game server"
  ],
  "author": "Jason Holt Smith <bicarbon8@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "phaser": "^3.60.0",
    "phaser-game-server": "^1.1.0",
    "phaser-game-server-engine": "^1.1.0",
    "socket.io": "^4.7.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "rimraf": "^5.0.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  }
}
