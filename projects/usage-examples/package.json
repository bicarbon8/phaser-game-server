{
  "name": "space-sim-server",
  "version": "1.0.0",
  "description": "example usage of the phaser-game-server projects",
  "type": "module",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc -b && webpack --env production",
    "build:dev": "npm run clean && tsc -b && webpack",
    "start": "npm run build && npm run open",
    "start:dev": "npm run build:dev && npm run open",
    "open": "npx phaser-game-server",
    "test": ""
  },
  "keywords": [
    "phaser",
    "html game",
    "typescript game",
    "example game server"
  ],
  "author": "Jason Holt Smith <bicarbon8@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "phaser": "^3.55.2",
    "phaser-game-server": "^1.0.0",
    "phaser-game-server-engine": "^1.0.0",
    "socket.io": "^4.5.4"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "file-replace-loader": "^1.4.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
